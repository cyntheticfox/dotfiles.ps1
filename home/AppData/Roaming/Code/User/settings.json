{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "python.insidersChannel": "off",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.fontFamily": "'CaskaydiaCove NF', Monospace",
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "editor.rulers": [
    80,
    120
  ],
  "editor.tabCompletion": "onlySnippets",
  "editor.wrappingIndent": "indent",
  "editor.cursorBlinking": "smooth",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.bibtex-format.tab": "4 spaces",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "C:\\Program Files\\MiKTeX 2.9\\miktex\\bin\\x64\\latexmk.exe",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "lualatexmk",
      "command": "C:\\Program Files\\MiKTeX 2.9\\miktex\\bin\\x64\\latexmk.exe",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "latexmk_rconly",
      "command": "C:\\Program Files\\MiKTeX 2.9\\miktex\\bin\\x64\\latexmk.exe",
      "args": [
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "C:\\Program Files\\MiKTeX 2.9\\miktex\\bin\\x64\\pdflatex.exe",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "C:\\Program Files\\MiKTeX 2.9\\miktex\\bin\\x64\\bibtex.exe",
      "args": [
        "%DOCFILE%"
      ],
      "env": {}
    },
    {
      "name": "rnw2pdf",
      "command": "Rscript",
      "args": [
        "-e",
        "knitr::knit2pdf('%DOCFILE%')"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.autoBuild.run": "never",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.alwaysSignOff": true,
  "git.autofetch": true,
  "git.enableCommitSigning": true,
  "workbench.iconTheme": "vscode-icons",
  "latex-workshop.message.update.show": false,
  "redhat.telemetry.enabled": true,
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.windows": "C:\\Users\\david\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe",
    "vscode-kubernetes.minikube-path.windows": "C:\\Users\\david\\.vs-kubernetes\\tools\\minikube\\windows-amd64\\minikube.exe"
  },
  "tabnine.experimentalAutoImports": true,
  "editor.fontSize": 12,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "workbench.colorTheme": "One Dark Pro",
  "remote.SSH.remotePlatform": {
    "windev": "windows"
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ]
}
